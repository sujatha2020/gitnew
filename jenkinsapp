pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                echo 'making the jenkins app'
            }
        }
        stage('Test') { 
            steps {
                echo 'testing the jenkins app'
            }
        }
        stage('Deploy') { 
            steps {
                echo 'deploying the jenkins app'
            }
        }
    }
}


